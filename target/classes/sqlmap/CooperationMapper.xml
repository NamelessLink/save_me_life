<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lxs.dao.CooperationMapper" >
  <resultMap id="BaseResultMap" type="com.lxs.entity.CooperationKey" >
    <id column="driver_id" property="driverId" jdbcType="VARCHAR" />
    <id column="r_id" property="rId" jdbcType="VARCHAR" />
  </resultMap>
  <select id="selectByRid" resultMap="BaseResultMap">
    select *
    from cooperation
    where r_id = #{r_id}
  </select>
  <delete id="deleteByPrimaryKey" parameterType="com.lxs.entity.CooperationKey" >
    delete from cooperation
    where driver_id = #{driverId,jdbcType=VARCHAR}
      and r_id = #{rId,jdbcType=VARCHAR}
  </delete>
  <insert id="insert" parameterType="com.lxs.entity.CooperationKey" >
    insert into cooperation (driver_id, r_id)
    values (#{driverId,jdbcType=VARCHAR}, #{rId,jdbcType=VARCHAR})
  </insert>
  <insert id="insertSelective" parameterType="com.lxs.entity.CooperationKey" >
    insert into cooperation
    <trim prefix="(" suffix=")" suffixOverrides="," >
      <if test="driverId != null" >
        driver_id,
      </if>
      <if test="rId != null" >
        r_id,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides="," >
      <if test="driverId != null" >
        #{driverId,jdbcType=VARCHAR},
      </if>
      <if test="rId != null" >
        #{rId,jdbcType=VARCHAR},
      </if>
    </trim>
  </insert>
</mapper>